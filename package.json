{
  "name": "campus-navigation",
  "version": "1.0.0",
  "description": "Campus Indoor-Outdoor Navigation System",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "pnpm -r test",
    "type-check": "pnpm -r type-check",
    "ci:quality": "pnpm run lint && pnpm run type-check && pnpm audit --audit-level moderate",
    "ci:test": "pnpm run test",
    "ci:build": "pnpm run build",
    "prepare": "husky install"
  },
  "keywords": [
    "navigation",
    "campus",
    "indoor",
    "qr-code",
    "pathfinding"
  ],
  "author": "Campus Navigation Team",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
